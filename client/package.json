{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.4.16",
        "@babel/preset-react": "^7.16.7",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@mui/icons-material": "^5.3.0",
        "@mui/lab": "^5.0.0-alpha.65",
        "@mui/material": "^5.3.0",
        "@mui/styles": "^5.3.0",
        "axios": "^0.25.0",
        "env-cmd": "^10.1.0",
        "graphql": "^16.2.0",
        "graphql-ws": "^5.5.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.0",
        "subscriptions-transport-ws": "^0.11.0",
        "typescript": "^4.5.4",
        "use-async-effect": "^2.2.5",
        "web-vitals": "^2.1.3"
    },
    "dependenciesComments": {
        "@apollo/client": "can't be upgraded past 3.2.9 until https://github.com/apollographql/apollo-client/issues/7608#issuecomment-914530291 is resolved"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "generate": "env-cmd -f ../.env graphql-codegen --config codegen.yml"
    },
    "eslintConfig": {
        "root": true,
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "project": "./tsconfig.json"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "extends": [
            "prettier",
            "react-app"
        ],
        "rules": {
            "@typescript-eslint/no-dynamic-delete": "off",
            "@typescript-eslint/no-non-null-assertion": "off",
            "@typescript-eslint/no-unused-vars": "off",
            "@typescript-eslint/restrict-template-expressions": "off",
            "no-void": "off",
            "react/jsx-uses-react": "off",
            "react/react-in-jsx-scope": "off",
            "react-hooks/exhaustive-deps": 0
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.3.1",
        "@graphql-codegen/introspection": "^2.1.1",
        "@graphql-codegen/schema-ast": "^2.4.1",
        "@graphql-codegen/typescript": "^2.4.2",
        "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.9",
        "@graphql-codegen/typescript-operations": "^2.2.2",
        "@graphql-codegen/typescript-react-apollo": "^3.2.4",
        "@types/lodash": "^4.14.178",
        "@types/node": "^17.0.10",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "prettier": "^2.5.1",
        "prettier-plugin-organize-imports": "^2.3.4",
        "sass": "^1.49.0",
        "ts-node": "^10.4.0"
    }
}
